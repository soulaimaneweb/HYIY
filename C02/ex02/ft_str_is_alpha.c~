int	ft_str_is_numeric(char *str)
{
	int	i;

	i = 0;
	while (str[i] != '\0')
	{
		if (!((str[i] >= 'A' && str[i] <= 'z') ||
			(str[i] >= 'a' && str[i] <= 'z')))
		{

		}
	}
}
